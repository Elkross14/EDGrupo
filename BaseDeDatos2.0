CREATE DATABASE NotasEntornos;

USE NotasEntornos;

-- DROP TABLE Alumnos;
CREATE TABLE Alumnos (
	idAlumno INT UNIQUE NOT NULL AUTO_INCREMENT,
    nombre VARCHAR(51),
    primerApellido VARCHAR(51),
	segundoApellido VARCHAR(51),
    notaFinal DECIMAL(4,2),
    CONSTRAINT pk_alu_idAlu PRIMARY KEY (idAlumno) 
);

